{
  "phases": {
    "package": {
      "actions": [
        {
          "action_id": "helm-package"
        },
        {
          "action_id": "helm-lint"
        }
      ],
      "context": {
        "resources": {
          "path": "/bitnami/thanos",
          "url": "{SHA_ARCHIVE}"
        }
      }
    },
    "verify": {
      "actions": [
        {
