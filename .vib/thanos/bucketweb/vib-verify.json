            "vuln_type": [
              "OS"
            ]
          }
        },
        {
          "action_id": "health-check",
          "params": {
            "app_protocol": "HTTP",
            "endpoint": "lb-thanos-bucketweb-http"
          }
        },
        {
          "action_id": "cypress",
          "params": {
            "app_protocol": "HTTP",
            "endpoint": "lb-thanos-bucketweb-http",
            "resources": {
              "path": "/.vib/thanos/bucketweb/cypress"
            }
          }
        }
      ],
      "context": {
        "resources": {
          "path": "/bitnami/thanos",
          "url": "{SHA_ARCHIVE}"
        },
