{
  "phases": {
    "package": {
      "actions": [
        {
          "action_id": "helm-package"
        },
        {
          "action_id": "helm-lint"
        }
      ],
      "context": {
        "resources": {
          "path": "/bitnami/redmine",
          "url": "{SHA_ARCHIVE}"
        }
      }
    },
    "verify": {
      "actions": [
        {
          "action_id": "trivy",
          "params": {
            "threshold": "CRITICAL",
            "vuln_type": [
              "OS"
            ]
          }
        },
        {
          "action_id": "health-check",
          "params": {
            "app_protocol": "HTTP",
            "endpoint": "lb-redmine-http"
          }
        },
        {
          "action_id": "goss",
          "params": {
            "resources": {
              "path": "/.vib/redmine/goss"
            },
            "remote": {
              "workload": "deploy-redmine"
            }
          }
        },
        {
          "action_id": "cypress",
          "params": {
            "resources": {
              "path": "/.vib/redmine/cypress"
            },
            "endpoint": "lb-redmine-http",
            "app_protocol": "HTTP",
            "env": {
              "username": "user",
              "password": "ComplicatedPassword123!4"
            }
          }
        }
      ],
      "context": {
        "resources": {
          "path": "/bitnami/redmine",
          "url": "{SHA_ARCHIVE}"
        },
        "runtime_parameters": "InJlZG1pbmVQYXNzd29yZCI6ICJVeWtLQTRTdyIKInJlZG1pbmVVc2VybmFtZSI6ICJ1c2VyIgoic2VydmljZSI6CiAgInBvcnQiOiA4MAogICJ0eXBlIjogIkxvYWRCYWxhbmNlciI=",
        "target_platform": {
          "size": {
            "name": "S4"
          },
          "target_platform_id": "{VIB_ENV_ALTERNATIVE_TARGET_PLATFORM}"
        }
      }
    }
  }
}
